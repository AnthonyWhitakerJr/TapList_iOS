platform :ios, '10.0'

target 'TapList' do
  use_frameworks!

  inhibit_all_warnings!

  # Pods for TapList
  pod 'Alamofire', '~> 4.0'
  pod 'Firebase/Core', :inhibit_warnings => true
  pod 'Firebase/Auth', :inhibit_warnings => true
  pod 'Firebase/Database', :inhibit_warnings => true

  target 'TapListTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Alamofire', '~> 4.0'
    pod 'Firebase/Core', :inhibit_warnings => true
    pod 'Firebase/Auth', :inhibit_warnings => true
    pod 'Firebase/Database', :inhibit_warnings => true
  end

  target 'TapListUITests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Alamofire', '~> 4.0'
    pod 'Firebase/Core', :inhibit_warnings => true
    pod 'Firebase/Auth', :inhibit_warnings => true
    pod 'Firebase/Database', :inhibit_warnings => true
  end

end
